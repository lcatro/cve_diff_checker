From 27b9bf644bcf1208f7d6594bdd104cc8a8bb0646 Mon Sep 17 00:00:00 2001
From: Even Rouault <even.rouault@spatialys.com>
Date: Wed, 5 Jun 2019 19:04:07 +0200
Subject: [PATCH] KML: set OAMS_TRADITIONAL_GIS_ORDER for SRS returned on
 returned layers

---
 gdal/ogr/ogrsf_frmts/kml/ogrkmllayer.cpp | 1 +
 1 file changed, 1 insertion(+)

diff --git a/gdal/ogr/ogrsf_frmts/kml/ogrkmllayer.cpp b/gdal/ogr/ogrsf_frmts/kml/ogrkmllayer.cpp
index d50be6d9d24..2a79da1e8b0 100644
--- a/gdal/ogr/ogrsf_frmts/kml/ogrkmllayer.cpp
+++ b/gdal/ogr/ogrsf_frmts/kml/ogrkmllayer.cpp
@@ -79,6 +79,7 @@ OGRKMLLayer::OGRKMLLayer( const char * pszName,
     if( poSRSIn != nullptr )
     {
         poSRS_->SetWellKnownGeogCS( "WGS84" );
+        poSRS_->SetAxisMappingStrategy(OAMS_TRADITIONAL_GIS_ORDER);
         if( !poSRS_->IsSame(poSRSIn) )
         {
             poCT_ = OGRCreateCoordinateTransformation( poSRSIn, poSRS_ );
