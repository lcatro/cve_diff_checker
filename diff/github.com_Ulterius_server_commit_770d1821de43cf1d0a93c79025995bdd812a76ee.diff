+        private Bitmap ExtractRect(int originX, int originY, int width, int height)
+        {
+            // Get the desktop capture screenTexture
+            var mapSource = _mDevice.ImmediateContext.MapSubresource(_desktopImageTexture, 0, MapMode.Read,
+                MapFlags.None);
+
